<h1>Sign Up</h1>
<form action="<%= users_url%>" method="POST">
    <%= auth_token %>
    <label> Email
        <input type="text" name="user[email]" value="<%= @user.email%>">
    </label>
    <label> Password
        <input type="password" name="user[password]" value="<%= @user.password%>">
    </label>
    <button>Submit</button>
</form>

